<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<SplitPane fx:id="splitPane" dividerPositions="0.32" prefHeight="500.0" prefWidth="800.0"
           xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/"
           fx:controller="com.cozyspace.librarymanagement.controller.CreateAccountSuccessController">
    <AnchorPane minWidth="256">
        <ImageView fitHeight="80" fitWidth="80" layoutX="25.0" layoutY="80.0">
            <Image url="@../book.png"/>
        </ImageView>
        <Label layoutX="25.0" layoutY="160.0" text="Hoàn tất!">
            <font>
                <Font size="25"/>
            </font>
        </Label>
    </AnchorPane>
    <AnchorPane minWidth="544" prefHeight="493.0" prefWidth="546.0">
        <Label layoutX="30.0" layoutY="137.0" prefHeight="28.0" text="Tài khoản của bạn đã được tạo thành công!">
            <font>
                <Font size="20"/>
            </font>
        </Label>
        <Button fx:id="nextButton" defaultButton="true" layoutX="364.0" layoutY="327.0" onAction="#next"
                prefHeight="36.0" prefWidth="108.0" text="Tiếp tục">
            <font>
                <Font size="15"/>
            </font>
        </Button>
    </AnchorPane>

</SplitPane>
